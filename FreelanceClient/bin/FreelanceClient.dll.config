  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_IUserService" />
        <binding name="BasicHttpBinding_IProjectService" />
        <binding name="BasicHttpBinding_IProjectRequestAlloted" />
